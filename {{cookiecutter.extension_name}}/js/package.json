{
  "name": "{{ cookiecutter.extension_name }}",
  "version": "0.0.1",
  "description": "{{ cookiecutter.project_short_description }}",
  "author": "{{ cookiecutter.author_name }}",
  "main": "lib/index.js",
  "license": "Apache-2.0",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "scripts": {
    "build": "babel src/ --source-maps --out-dir lib/",
    "build:lab": "rimraf ../{{ cookiecutter.extension_name }}/labextension && jupyter labextension build .",
    "build:all": "npm run build && npm run build:lab",
    "clean": "rimraf lib",
    "fix": "yarn lint --fix",
    "lint": "eslint -c .eslintrc.js --ext .js src/ tests/",
    "prepublishOnly": "npm run build",
    "test": "jest --coverage --collectCoverageFrom=src/*.{js}"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.css"
  ],
  "jupyterlab": {
    "extension": "lib/index.js",
    "outputDir": "../{{ cookiecutter.extension_name }}/labextension",
    "discovery": {
      "server": {
        "base": {
          "name": "{{ cookiecutter.extension_name }}"
        },
        "managers": [
          "pip"
        ]
      }
    }
  },
  "dependencies": {
    "@jupyterlab/application": "^3.2.4",
    "@jupyterlab/apputils": "^3.2.4",
    "@jupyterlab/coreutils": "^5.2.4",
    "@jupyterlab/docmanager": "^3.2.4",
    "@jupyterlab/filebrowser": "^3.2.4",
    "@jupyterlab/launcher": "^3.2.4",
    "requests-helper": "^0.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.16.4",
    "@jupyterlab/builder": "^3.2.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.4.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^27.4.0",
    "jest-raw-loader": "^1.0.1",
    "jest-transform-css": "^3.0.0",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2"
  }
}
